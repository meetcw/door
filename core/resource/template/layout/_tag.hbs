<!doctype html><html><head><meta charset="utf-8"><meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1"><title>标签|{{title}}</title><script defer="defer" src="/./static/assets/tag.js"></script><link href="/./static/assets/tag.css" rel="stylesheet"></head><body><div class="tag-wraper"><header class="header-wrapper"><a class="one" href=""><div class="logo" style="background-image:url({{logo}})"></div><div class="title"><h1>{{title}}</h1></div></a><div class="toggle"><i class="iconfont icon-menu"></i></div><ul class="menu">{{#each menus}}<li><a href="{{path}}" target="{{target}}">{{title}}</a></li>{{/each}}</ul></header><div class="content-wrapper"><div class="tags">{{#each tags}} <a href="#anchor_{{hash name}}" class="tag">{{name}}({{count list}})</a> {{/each}}</div>{{#each tags}}<div><h2 id="anchor_{{hash name}}">{{name}}</h2><ul class="posts">{{#each list}}<li class="post"><div class="meta"><time>{{datetime create_time}}</time></div><span><a href="/post/{{id}}.html">{{title}}</a></span></li>{{/each}}</ul></div>{{/each}}</div><footer class="footer-wrapper"><div class="footer-left">Copyright © 2018 BAIYAN</div><div class="footer-right"><nav><ul>{{#each menus}}<li><a href="{{path}}" target="{{target}}">{{title}}</a></li>{{/each}}</ul></nav></div></footer></div></body></html>